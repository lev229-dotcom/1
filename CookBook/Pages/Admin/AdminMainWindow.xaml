        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:CookBook.Pages.Admin"
        mc:Ignorable="d"
        ResizeMode="NoResize"
        Title="Главное меню" Height="600" Width="1000">
